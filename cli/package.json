{
  "name": "@devops-pulumi-ts/cli",
  "version": "1.0.0",
  "description": "CLI for deploying containerized apps to GCP Cloud Run or Azure Container Apps",
  "type": "module",
  "bin": {
    "devops-deploy": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "execa": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.39.2",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
